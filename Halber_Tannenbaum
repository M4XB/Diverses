#include <iostream>
using namespace std;

int main()
{
    int rows = 8;
    int count = 1;
    for( int row = rows; row > 0; row = row - 1 ) {
        for( int column = 1; column < count; column = column +1){
            cout << "**";
        }
    cout << "*" << endl;
    count = count +1;
    }
    return 0;
}
